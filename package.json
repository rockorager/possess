{
    "name": "possess",
    "version": "0.0.1",
    "description": "A TypeScript library",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "lib"
    ],
    "scripts": {
        "build:native": "./scripts/build-native.sh",
        "build": "bun run build:native && bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly",
        "typecheck": "tsc --noEmit",
        "test": "cd native && zig build test && cd .. && bun test",
        "bench": "bun run src/bulk-cell.bench.ts",
        "lint": "eslint src examples",
        "format": "zig fmt native && prettier --write .",
        "check": "bun run typecheck && bun run lint && bun run format",
        "prepublishOnly": "bun run check && bun run test && bun run build",
        "example": "bun run examples/basic.ts",
        "example:node": "bun run build && node examples/basic-node.js"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@typescript-eslint/eslint-plugin": "^8.45.0",
        "@typescript-eslint/parser": "^8.45.0",
        "eslint": "^9.36.0",
        "globals": "^16.4.0",
        "mitata": "^1.0.34",
        "prettier": "^3.6.2",
        "typescript": "^5"
    },
    "keywords": [],
    "author": "",
    "license": "MIT"
}
